<?xml version="1.0" encoding="UTF-8"?>
<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
  <services>
    <service id="Nucleos\Doctrine\EventListener\ORM\ConfirmableListener">
        <tag name="doctrine.event_subscriber" />
    </service>
      <service id="Nucleos\Doctrine\EventListener\ORM\DeletableListener">
          <tag name="doctrine.event_subscriber" />
      </service>
      <service id="Nucleos\Doctrine\EventListener\ORM\LifecycleDateListener">
          <tag name="doctrine.event_subscriber" />
      </service>
      <service id="Nucleos\Doctrine\EventListener\ORM\SortableListener">
          <tag name="doctrine.event_subscriber" />
          <argument type="service" id="property_accessor" />
      </service>
      <service id="Nucleos\Doctrine\EventListener\ORM\UniqueActiveListener">
          <tag name="doctrine.event_subscriber" />
          <argument type="service" id="property_accessor" />
      </service>
      <service id="Nucleos\Doctrine\EventListener\ORM\TablePrefixEventListener">
          <tag name="doctrine.event_subscriber" />
          <argument>%nucleos_doctrine.table.prefix%</argument>
      </service>
  </services>
</container>
